# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
# Set default charset
[*]
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.java]
indent_style = space
indent_size = 4


[*.xml]
indent_style = space
indent_size = 4

[*.properties]
indent_size = 0

[*.yaml]
indent_style = space
indent_size = 4

